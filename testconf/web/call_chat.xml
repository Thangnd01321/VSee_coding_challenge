<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UI Automation Test" verbose="1"
       parallel="classes" thread-count="4" configfailurepolicy="continue">
    <listeners>
        <listener class-name="reporter.ExtendReporterTestListener"/>
    </listeners>
    <test name="VSee call chat test" annotations="JDK" preserve-order="true">
        <parameter name="hub_url" value="http://192.168.1.14:4444"/>
        <classes>
            <class name="test.web.patient.PatientTest">
                <parameter name="browser" value="chrome"/>
                <parameter name="providerId" value="1"/>
                <parameter name="patientId" value="1"/>
            </class>
            <class name="test.web.provider.ProviderTest">
                <parameter name="browser" value="chrome"/>
                <parameter name="patientId" value="1"/>
                <parameter name="providerId" value="1"/>
            </class>
        </classes>
    </test>
</suite>